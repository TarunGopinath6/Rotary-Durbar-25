{
  "expo": {
    "name": "Royal Durbar",
    "slug": "rotary-club-durbar",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/durbar_512.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/durbar_1024_square.png",
      "supportsTablet": false,
      "bundleIdentifier": "com.dasorakel.rotaryclubdurbar"
    },
    "android": {
      "icon": "./assets/images/durbar_512.png",
      "package": "com.dasorakel.rotaryclubdurbar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/durbar_1024_square.png",
          "imageWidth": "75%",
          "resizeMode": "contain",
          "backgroundColor": "#a32638"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4da7b0f4-5eb7-478d-b9f5-6b3a6af2da4a"
      }
    }
  }
}
